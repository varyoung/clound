@import "./mixins.styl";

.dialog-plane
    >.el-dialog
        border-radius 10px
        .el-dialog__header
            .iconfont
                color #449dbf
            border-bottom 1px solid #f4f4f4
            text-align left 
            color #00000
            background #f4f4f4            
            border-top-left-radius 20px;
            border-top-right-radius 20px;

.system-template-panel
    common-panel(0,20px)
    flex-direction column
    >.system-inner-template
        position absolute
        left 30px
        right 30px
        top 0px
        bottom 0px