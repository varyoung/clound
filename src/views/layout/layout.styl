@import "../../resource/var.styl" 
  
html,body
    margin 0
    padding 0
    display flex
    flex 1 0 auto
    height 100%
    >.panel
        display flex
        flex 1 0 auto
        >.el-container
            flex-direction column
            >.el-container
                >.el-aside 
                    background-color #1c415e
                    color #333
                    text-align center
                >.el-container
                    flex-direction column
                    >.el-main
                        background-color $footer-color
                        color #333
                        text-align center
                        line-height 160px
  

  

  
