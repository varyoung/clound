.user-manage
    background-color #ffffff
    display flex
    flex 1 0 auto
    // flex-wrap wrap
    >.select-part
        padding 0 20px
        width 300px
        display flex
        flex 0 0 300px
        flex-direction column      
        // flex-wrap wrap      
    >.form-part
        display flex
        flex 0 1 auto
        flex-direction column
        margin-right 20px
        // flex-wrap wrap
        >.btn-group
            display flex
            flex 0 0 auto
            justify-content space-between
            align-items center
            >div
                display flex
                cursor pointer
        >.el-form--inline
            display flex
            flex 0 1 auto
            flex-wrap wrap
            >.el-form-item
                &:last-child
                    padding-left 20px