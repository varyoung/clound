@import "../../../resource/mixins.styl"

.attack-logs
    common-panel(0,20px)
    // flex-wrap wrap
    >.form-part
        width 100%
        display flex
        flex 0 1 auto
        flex-direction column
        >.el-form
            >.el-form-item
                >.el-form-item__label
                    width 110px  !important
                >.el-form-item__content
                    >.dateRange 
                        width 316px !important 
                        padding-top 2px
                        >.el-range__close-icon
                            position absolute   
        .el-input__inner
            width 200px
        .el-date-editor
            width auto !important
        // margin-right 20px
        // flex-wrap wrap
        >.btn-group
            display flex
            flex 0 0 auto
            justify-content space-between
            align-items center
            >div
                display flex
                cursor pointer
        >.el-form--inline
            display flex
            flex 0 1 auto
            flex-wrap wrap
            >.el-form-item
                &:last-child
                    padding-left 20px
        >.cloud-table
            >.table-panel
                >.websiteOperate
                    display flex
                    >i
                        font-size 14px            
